-if=\"this.user_role_id == 1 || this.user_role_id == 2\">\r\n              <router-link to=\"/dashboard/surveyFirm\">\r\n                <span class=\"material-icons-sharp\">add</span>\r\n                Survey Firm\r\n              </router-link>\r\n            </li>\r\n          </ul>\r\n        </li>\r\n      </div>\r\n    </aside>\r\n  </div>\r\n</template>\r\n<script>\r\nexport default {\r\n  name: \"SideBar\",\r\n  props: [\"isOpen\"],\r\n  data() {\r\n    return {\r\n      isdisplay: false,\r\n      isdisplay1: false,\r\n      isdisplay2: false,\r\n      isdisplay3: false,\r\n      isdisplay4: false,\r\n      user_role: JSON.parse(localStorage.getItem(\"user_role\")),\r\n      user_role_id: JSON.parse(localStorage.getItem(\"user_role_id\")),\r\n    };\r\n  },\r\n  mounted() {\r\n    console.log(\"user_role: \" + this.user_role);\r\n    console.log(\"user_role_id: \" + this.user_role_id);\r\n  },\r\n  methods: {\r\n    menuDropdown() {\r\n      this.isdisplay = !this.isdisplay;\r\n    },\r\n\r\n    menuDropdown1() {\r\n      this.isdisplay1 = !this.isdisplay1;\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n#sidebar{\r\n  background-color: rgb(31, 59, 72);\r\n  margin-right: 2%;\r\n  /* border-radius: 2%; */\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}