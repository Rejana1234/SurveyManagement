ssword.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <section class=\"container\">\r\n            <div class=\"form\">\r\n                <div class=\"form-content\">\r\n                    <header>\r\n                        <img src=\"../../assets/Logo/1.svg\" alt=\"\" style=\"width: 100px\">\r\n                    </header>\r\n                    <form v-on:submit.prevent=\"changePassword\">\r\n                        <div class=\"field input-field\">\r\n                            <input type=\"email\" name=\"email\" v-model=\"change_password_data.email\" placeholder=\"Your email\" class=\"input\">\r\n                        </div>\r\n                        <div class=\"field input-field\">\r\n                            <input type=\"password\" name=\"password\" v-model=\"change_password_data.password\" placeholder=\"New Password\" class=\"input\">\r\n                        </div>\r\n                        <div class=\"field button-field\">\r\n                            <button type=\"submit\">Save</button>\r\n                        </div>\r\n                    </form>\r\n\r\n                </div>\r\n            </div>\r\n        </section>\r\n\r\n    </div>\r\n</template>\r\n<script>\r\n    import {mapState} from 'vuex';\r\n\r\n    export default {\r\n        name: 'MyLogin',\r\n        title: 'InterPort - Reset Password',\r\n\r\n        data() {\r\n            return {\r\n                change_password_data: {\r\n                    email: '',\r\n                    password: '',\r\n                }\r\n            }\r\n        },\r\n\r\n        computed: {\r\n            ...mapState({\r\n                message: state => state.success_message\r\n            })\r\n        },\r\n\r\n        methods: {\r\n            changePassword: async function(){\r\n                try {\r\n                    let token = this.$route.params.token;\r\n                    let formData = new FormData();\r\n\r\n                    formData.append('email', 