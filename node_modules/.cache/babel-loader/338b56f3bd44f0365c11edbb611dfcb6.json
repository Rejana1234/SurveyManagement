nk>\r\n          </div>\r\n        </form>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapState, mapActions } from \"vuex\";\r\n\r\nexport default {\r\n  name: \"AddCategory\",\r\n\r\n  data() {\r\n    return {\r\n      errors: {},\r\n    };\r\n  },\r\n\r\n  computed: {\r\n    ...mapState({\r\n      edit_category: (state) => state.category.category,\r\n      message: (state) => state.category.success_message,\r\n    }),\r\n  },\r\n\r\n  mounted() {\r\n    this.getEditCategory(this.$route.params.id);\r\n  },\r\n\r\n  methods: {\r\n    ...mapActions({\r\n      getEditCategory: \"category/edit_category\",\r\n    }),\r\n\r\n    editCategory: async function () {\r\n      try {\r\n        let id = this.$route.params.id;\r\n        let formData = new FormData();\r\n\r\n        formData.append(\"name\", this.edit_category.name);\r\n        formData.append(\"description\", this.edit_category.description);\r\n\r\n        formData.append(\"_method\", \"PUT\");\r\n\r\n        await this.$store\r\n          .dispatch(\"category/update_category\", { id: id, data: formData })\r\n          .then(() => {\r\n            this.$swal.fire({\r\n              toast: true,\r\n              position: \"top-end\",\r\n              icon: \"success\",\r\n              title: this.message,\r\n              showConfirmButton: false,\r\n              timer: 1500,\r\n            });\r\n\r\n            this.getEditCategory(this.$route.params.id);\r\n          });\r\n      } catch (e) {\r\n        console.log(e);\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n#edit-category {\r\n  padding: 0px 5px 0px 5px;\r\n  margin: 0px 15px 0px 15px;\r\n}\r\n.card {\r\n  background-color: #fff;\r\n  padding: 15px;\r\n  border-radius: 10px;\r\n  box-shadow: #f1f1f1;\r\n  margin-top: 25px;\r\n}\r\n* {\r\n  box-sizing: border-box;\r\n  appearance: auto;\r\n}\r\n\r\nselect {\r\n  width: 100%;\r\n  padding: 12px;\r\n  border: 1px solid #ccc;\r\n  bord